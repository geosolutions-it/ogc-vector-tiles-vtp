{
    "proxyUrl": {
        "url": "",
        "useCORS": [],
        "autoDetectCORS": true
    },
    "geoStoreUrl": "",
    "printUrl": "",
    "themePrefix": "vtp2",
    "translationsPath": "./MapStore2/web/client/translations",
    "projectionDefs": [
        {
            "code": "EPSG:3395",
            "def": "+proj=merc +lon_0=0 +k=1 +x_0=0 +y_0=0 +datum=WGS84 +units=m +no_defs",
            "extent": [
                -20026376.39,
                -15496570.74,
                20026376.39,
                18764656.23
            ],
            "worldExtent": [
                -180.0,
                -80.0,
                180.0,
                84.0
            ]
        }
    ],
    "initialState": {
        "defaultState": {
            "maptype": {
                "mapType": "openlayers"
            }
        }
    },
    "plugins": {
        "viewer": [
            "Layout",
            {
                "name": "Map",
                "cfg": {
                    "mapType": "openlayers",
                    "mapOptions": {
                        "openlayers": {
                            "interactions": {
                                "pinchRotate": false,
                                "altShiftDragRotate": false
                            }
                        }
                    }
                }
            },
            "Layers",
            "OGCAPICatalog",
            "LayerSettings",
            "LayerFilter",
            {
                "name": "MapFooter",
                "override": {
                    "Layout": {
                        "container": "footer",
                        "priority": 1
                    }
                }
            },
            {
                "name": "CRSSelector",
                "cfg": {
                    "additionalCRS": {
                        "EPSG:3395": {
                            "label": "WorldMercatorWGS84Quad"
                        }
                    },
                    "crsProperties": {
                        "EPSG:3395": {
                            "label": "WorldMercatorWGS84Quad"
                        },
                        "EPSG:4326": {
                            "label": "WorldCRS84Quad"
                        },
                        "EPSG:3857": {
                            "label": "WebMercatorQuad"
                        }
                    },
                    "filterAllowedCRS": [
                        "EPSG:4326",
                        "EPSG:3857"
                    ]
                }
            },
            {
                "name": "Toolbar",
                "id": "NavigationBar",
                "override": {
                    "Layout": {
                        "container": "body",
                        "priority": 1
                    }
                },
                "cfg": {
                    "id": "navigationBar"
                }
            },
            "MapLoading",
            {
                "name": "ZoomIn",
                "override": {
                    "Toolbar": {
                        "alwaysVisible": true
                    }
                }
            },
            {
                "name": "ZoomOut",
                "override": {
                    "Toolbar": {
                        "alwaysVisible": true
                    }
                }
            },
            {
                "name": "ZoomAll",
                "override": {
                    "Toolbar": {
                        "alwaysVisible": true
                    }
                }
            },
            {
                "name": "BackgroundSelector",
                "override": {
                    "Layout": {
                        "container": "body",
                        "priority": 1
                    }
                }
            },
            {
                "name": "OmniBar", 
                "override": {
                    "Layout": {
                        "container": "header",
                        "priority": 1
                    }
                }
            },
            "BurgerMenu",
            "MapImport",
            "MapExport",
            "HelpLink",
            "Logo",
            "ScaleBox"
        ]
    }
}