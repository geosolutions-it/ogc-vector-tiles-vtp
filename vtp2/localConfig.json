{
    "proxyUrl": "/mapstore/proxy/?url=",
    "geoStoreUrl": "",
    "printUrl": "",
    "themePrefix": "vtp2",
    "translationsPath": "./MapStore2/web/client/translations",
    "initialState": {
        "defaultState": {
            "maptype": {
                "mapType": "openlayers"
            }
        }
    },
    "plugins": {
        "viewer": [
            "Layout",
            {
                "name": "Map",
                "cfg": {
                    "mapType": "openlayers",
                    "mapOptions": {
                        "openlayers": {
                            "interactions": {
                                "pinchRotate": false,
                                "altShiftDragRotate": false
                            }
                        }
                    }
                }
            },
            "Layers"
        ]
    }
}